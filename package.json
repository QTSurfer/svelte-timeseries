{
  "private": true,
  "name": "qtsurfer-svelte-timeseries",
  "version": "0.0.1",
  "description": "QTSurfer svelte timeseries ui monorepo",
  "license": "Apache-2.0",
  "author": "QTSurfer (https://github.com/QTSurfer)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QTSurfer/svelte-timeseries.git"
  },
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "pnpm -r --filter=./packages/* build",
    "build:examples": "pnpm -r --filter=./examples/* build",
    "changeset:publish": "changeset publish",
    "changeset:version": "changeset version",
    "check": "pnpm -r check",
    "ci:install": "pnpm install --frozen-lockfile --prefer-offline",
    "clean": "pnpm -r clean",
    "clear": "pnpm -r clear",
    "dev:charts": "pnpm -F @qtsurfer/sveltecharts dev",
    "dev:ts": "pnpm -F @qtsurfer/svelte-timeseries dev",
    "format": "run-p format:all format:package",
    "format:all": "pnpm -r --filter=./packages/* format",
    "format:package": "npx prettier-package-json --write ./package.json",
    "preinstall": "npx only-allow pnpm",
    "lint": "pnpm -r --filter=./packages/* lint",
    "publish": "pnpm -r publish --access public --no-git-checks",
    "publish:dry": "pnpm -r publish --access public --no-git-checks --dry-run",
    "test": "pnpm -r --filter=./packages/* test"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0"
  },
  "keywords": [
    "charts",
    "finance",
    "svelte",
    "sveltekit",
    "timeseries",
    "visualization"
  ],
  "engines": {
    "pnpm": ">=10.18.0"
  },
  "packageManager": "pnpm@10.18.0"
}
